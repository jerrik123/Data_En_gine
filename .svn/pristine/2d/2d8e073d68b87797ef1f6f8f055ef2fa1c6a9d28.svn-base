# config path ,default classes path/config
CONFIG_PATH=
#file name regex VALIDATION
SYS_ACTION_REGEX_BOOK=action.+.xml
SYS_DEPEND_REGEX_BOOK = depend.+.xml
SYS_REMOTE_REGEX_BOOK = remote.+.xml
#charset
CHARSET=UTF-8

#remote bulid config
REMOTE_BULID_IP_BOOK=172.0.0.1
REMOTE_BULID_NAMING_BOOK=NSG
REMTE_BULID_PORT_BOOK=8888

#集团通讯的MQEngine
#DEV 开发环境
#mq.engine.http.url.prefix=http://localhost:8081/mq
#TEST 测试环境
mq.engine.http.url.prefix=http://10.10.7.58:8011/mq
#PRO 生产
#mq.engine.http.url.prefix=http://openapi.mangocity.com/mq

#查询集团积分
queryCrmEnablePoint.url.suffix=/point/queryCrmEnablePoint.action
#扣减集团积分
cutCrmPoint.url.suffix=/point/cutCrmPoint.action
#退还集团积分
increaseCrmPoint.url.suffix=/point/increaseCrmPoint.action

#请求集团接口设置超时,如果不设置,默认是60
http.timeout=20