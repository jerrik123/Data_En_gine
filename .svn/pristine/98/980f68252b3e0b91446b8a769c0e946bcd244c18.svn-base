<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper 
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mangocity.de.mbr.sqlmapper.point.pointWalletLogMapper">
	<select id="getPointWalletLogId" resultType="java.lang.Long">
		SELECT SEQ_MBR_ADDR_ID.NEXTVAL FROM DUAL
	</select>
		
	<insert id="insertPointWalletLog" parameterType="java.util.Map">
		INSERT INTO T_MBR_POINTWALLET_LOG(
			ID,
			MBR_ID,
			TRANS_NO,
			ORDER_CODE,
			PRODUCT_NAME,
			POINT_TYPE,
			POINT_VALUE,
			TRANS_TIME,
			TRANS_TYPE,
			TRANS_STATUS,
			ADJUST_TIME,
			REMARK
			) VALUES(
			#{id},
			#{mbrId},
			#{transNo},
			#{orderCode},
			#{productName},
			#{pointType},
			#{pointValue},
			#{transTime},
			#{transType},
			#{transStatus},
			#{adjustTime},
			#{remark},
			)
	</insert>
	
	<select id="selectPointWalletLogByOrdercodeAndType" parameterType="java.util.Map" resultType="java.util.Map"> 
		SELECT *
		FROM T_MBR_POINTWALLET_LOG
		WHERE 1=1
		<if test="mbrId != null and mbrId != ''">
			and MBR_ID = #{mbrId}
		</if>
		<if test="orderCode != null and orderCode != ''">
			and ORDER_CODE = #{orderCode}
		</if>
		<if test="transType != null and transType != ''">
			and TRANS_TYPE = #{transType}
		</if>
	</select>
</mapper> 